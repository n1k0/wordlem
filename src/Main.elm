module Main exposing (main)

import Browser
import Html exposing (..)
import Html.Attributes exposing (..)
import Html.Events exposing (..)
import Random
import Set exposing (Set)


type alias Model =
    { gameState : GameState
    , attempts : List Attempt
    , word : Maybe WordToFind
    , currentTry : UserInput
    , language : Language
    , error : Maybe String
    }


type Language
    = English
    | French

type GameState
    = Errored String
    | Ongoing
    | Won
    | Lost


type Letter
    = Unused Char
    | Correct Char
    | Misplaced Char


type alias Attempt =
    List Letter


type alias UserInput =
    String


type alias WordToFind =
    String


type Msg
    = NewGame
    | NewWord (Maybe WordToFind)
    | Submit (Maybe WordToFind)
    | UpdateTry UserInput
    | SwitchLanguage Language 


maxAttempts : Int
maxAttempts =
    6


init : Language -> () -> ( Model, Cmd Msg )
init lang _ =
    ( { gameState = Ongoing
      , attempts = []
      , word = Nothing
      , currentTry = ""
      , language = lang 
      , error = Nothing
      }
    , Random.generate NewWord (randomWord lang)
    )


randomWord : Language -> Random.Generator (Maybe WordToFind)
randomWord lang =
    let 
        words = getDictionnary lang
    in
    Random.int 0 (List.length words - 1)
        |> Random.andThen
            (\int ->
                words
                    |> List.indexedMap
                        (\index word ->
                            if index == int then
                                Just word

                            else
                                Nothing
                        )
                    |> List.filterMap identity
                    |> List.head
                    |> Random.constant
            )


try : Language -> WordToFind -> UserInput -> Result String Attempt
try lang word input =
    let
        words = getDictionnary lang
        ( wordChars, inputChars ) =
            ( String.toList word
            , input |> String.toLower |> String.trim |> String.toList
            )
    in
    if List.any (Char.isAlpha >> not) inputChars then
        Err "Word must contains only alphabetic letters"

    else if String.length input /= 5 then
        Err "Word must be 5 letters long"

    else if not (List.member (String.toLower input) words) then
        Err <| "Sorry, " ++ input ++ " must be a word known from our dictionnary"

    else
        Ok
            (List.map2
                (\a b ->
                    if a == b then
                        Correct a

                    else if List.member a wordChars then
                        Misplaced a

                    else
                        Unused a
                )
                inputChars
                wordChars
            )


hasWon : List Attempt -> Bool
hasWon attempts =
    case attempts of
        [] ->
            False

        last :: _ ->
            List.all
                (\letter ->
                    case letter of
                        Correct _ ->
                            True

                        _ ->
                            False
                )
                last


gameState : Model -> GameState
gameState model =
    if hasWon model.attempts then
        Won

    else if List.length model.attempts >= maxAttempts then
        Lost

    else
        Ongoing


update : Msg -> Model -> ( Model, Cmd Msg )
update msg ({ attempts, currentTry, language } as model) =
    case msg of
        NewGame ->
            init language ()

        NewWord (Just string) ->
            ( { model | word = Just string }, Cmd.none )

        NewWord Nothing ->
            ( { model | gameState = Errored "There was an issue while picking a word" }, Cmd.none )

        UpdateTry string ->
            ( { model | currentTry = string }, Cmd.none )

        SwitchLanguage lang ->
            update NewGame { model | language = lang }

        Submit (Just wordToFind) ->
            case try language wordToFind currentTry of
                Ok attempt ->
                    ( { model
                        | attempts = attempt :: attempts
                        , currentTry = ""
                        , error = Nothing
                      }
                    , Cmd.none
                    )

                Err error ->
                    ( { model | error = Just error }, Cmd.none )

        Submit Nothing ->
            ( model, Cmd.none )


charToText : Char -> Html Msg
charToText =
    Char.toUpper >> List.singleton >> String.fromList >> text


viewAttempt : Attempt -> Html Msg
viewAttempt attempt =
    attempt
        |> List.map
            (\letter ->
                case letter of
                    Misplaced char ->
                        td [ class "letter misplaced bg-warning" ] [ charToText char ]

                    Correct char ->
                        td [ class "letter correct bg-success" ] [ charToText char ]

                    Unused char ->
                        td [ class "letter unused bg-secondary" ] [ charToText char ]
            )
        |> tr []


unusedLetters : List Attempt -> Set Char
unusedLetters =
    List.foldl
        (\attempt unused ->
            attempt
                |> List.filterMap
                    (\letter ->
                        case letter of
                            Unused char ->
                                Just char

                            _ ->
                                Nothing
                    )
                |> Set.fromList
                |> Set.union unused
        )
        (Set.fromList [])


newGameButton : Html Msg
newGameButton =
    p []
        [ button [ class "btn btn-primary", onClick NewGame ]
            [ text "Start new game" ]
        ]

languageButton : Language -> Bool -> Html Msg
languageButton lang isActive =
    let classes = if isActive then
                     "btn btn-secondary active"
                  else
                     "btn btn-secondary"
    in
    label [ class classes ]
          [ input [ name "lang", type_ "radio", onClick (SwitchLanguage lang), checked isActive ] []
          , text " ", text (toString lang)
          ]

viewUnusedLetters : List Attempt -> Html Msg
viewUnusedLetters attempts =
    let
        unused =
            attempts |> unusedLetters |> Set.toList
    in
    if List.length unused > 0 then
        div [ class "mb-3" ]
            [ h4 [ class "mb-3" ] [ text "Unused letters" ]
            , unused
                |> List.map (charToText >> List.singleton >> code [])
                |> List.intersperse (text ", ")
                |> div []
            ]

    else
        text ""


view : Model -> Html Msg
view ({ error, attempts, word, currentTry, language } as model) =
    div []
        [ 
          div [ class "btn-group btn-group-toggle", attribute "data-toggle" "buttons" ]
            [ languageButton English (language == English)
            , languageButton French (language == French)
            ]
        , p []
            [ text "Guess a 5 letters ", text (toString language), text " word in "
            , strong [] [ text <| String.fromInt maxAttempts ]
            , text " attempts or less!"
            ]
        , attempts
            |> List.reverse
            |> List.map viewAttempt
            |> table [ class "table" ]
        , case gameState model of
            Errored gameError ->
                div []
                    [ h3 [] [ text "The game encountered an error" ]
                    , text gameError
                    , newGameButton
                    ]

            Won ->
                div []
                    [ h3 [] [ text "You have won!" ]
                    , newGameButton
                    ]

            Lost ->
                div []
                    [ h3 [] [ text "Bummer!" ]
                    , case word of
                        Just wordToFind ->
                            p [] [ text "The word to find was ", strong [] [ text wordToFind ], text "." ]

                        Nothing ->
                            text ""
                    , viewUnusedLetters attempts
                    , newGameButton
                    ]

            Ongoing ->
                div []
                    [ viewUnusedLetters attempts
                    , case error of
                        Just error_ ->
                            p [ style "color" "red" ] [ text error_ ]

                        Nothing ->
                            text ""
                    , Html.form [ class "input-group", onSubmit (Submit word) ]
                        [ input
                            [ type_ "text"
                            , class "form-control"
                            , maxlength 5
                            , onInput UpdateTry
                            , value currentTry
                            ]
                            []
                        , button [ class "btn btn-primary" ] [ text "Submit" ]
                        ]
                    , div [ class "form-text" ] [ text "Enter a 5 letters ", text (toString language), text " word" ]
                    ]
        ]


main : Program () Model Msg
main =
    Browser.element
        { init = init English
        , view = view
        , update = update
        , subscriptions = always Sub.none
        }

toString : Language -> String
toString language = 
    case language of  
        English -> "english"
        French -> "french"

getDictionnary : Language -> List String
getDictionnary language =
    case language of  
        English -> wordsEn
        French -> wordsFr

